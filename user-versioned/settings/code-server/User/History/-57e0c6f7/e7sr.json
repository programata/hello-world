    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Kedro run",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/test/test_project/src/test_project/run.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}/src/test/test_project",
